    <script src="https://player.vimeo.com/api/player.js"></script>

    <script>
        const redirect = `<%=devFun%>`
        const previewURL = `<%= latestCourse.data().previewLink %> `
        const player = document.querySelector('#player');
        var options = {
            url: previewURL,
            responsive: true,
            background: true
            // playsinline: false,
            // controls: false,
            // autoplay: true
        };
        var videoPlayer = new Vimeo.Player('player', options);
        const btnPlay = document.querySelector('#playBtn')
        let togglePlay = true

        if (btnPlay) {
            btnPlay.addEventListener('click', () => {
                if (!togglePlay) {
                    videoPlayer.play()
                    togglePlay = true
                }
                else {
                    videoPlayer.pause()
                    togglePlay = false
                }
            })
        }
        videoPlayer.on('play', function () {
        });
    </script>