<% layout('/template/boilerplate') -%>




    <div class="mainDiv" style="position:relative;">
        <div style="width: 100vw;  position:relative;" id="player">
            <i id="playBtn" class="fas fa-play-circle"></i>

        </div>
        <div class="mainOverlay">
            <div class="course-content">
                <div class="language">Language : <%= latestCourse.data().courseLanguage %>
                </div>
                <div class="course-title">
                    <%= latestCourse.data().courseName %>
                </div>
                <div class="details">
                    <div class="instructor-name">
                        <%= latestCourse.data().instructorName %>
                    </div>
                    <div class="duration">Duration : <%= durationUnit(latestCourse.data().duration) %>
                    </div>
                </div>
                <div class="description">
                    <%= latestCourse.data().courseDescription %>
                </div>
                <div class="tags">
                    <div class="tag" <%if( latestCourse.data().bestsellerTag===null){%>
                        hidden <% } %>> Bestseller </div>
                    <div class="tag" <%if( latestCourse.data().beginnerTag===null){%> hidden
                        <% } %>> Beginner
                    </div>
                    <div class="tag" <%if( latestCourse.data().intermediateTag===null){%>
                        hidden <% } %>> Intermediate </div>
                </div>
            </div>
            <div class="cta">
                <a class="buyLink"><button>BUY NOW @<%= latestCourse.data().coursePrice-latestCourse.data().discountAmount %>
                            /-</button></a>
            </div>
        </div>
    </div>

    <div class="courseDiv">
        <div class="sectionHead">
            <h3>
                <%= latestCourse.data().courseName %>
            </h3>
            <div class="details">
                <div class="hours">
                    <%= durationUnit(latestCourse.data().duration) %>
                </div>
                <div class="modules">
                    <%= latestCourse.data().numModules %> Modules
                </div>
                <div class="lecs">
                    <%= latestCourse.data().numLectures %> Lectures
                </div>
            </div>
        </div>
        <% modules.forEach(module=> { %>
            <div class="courseRow">
                <div class="module">
                    <div class="moduleHead">
                        <div class="moduleName">
                            <%= module.data().moduleOrder %>. <%= module.data().moduleName %>
                        </div>
                        <div class="moduleDuration">
                            <%=durationUnit( module.data().duration) %>
                        </div>
                    </div>
                    <div class="moduleDesc">
                        <%= module.data().moduleDescription %>
                    </div>
                </div>
                <div class="vids">
                    <div class="owl-carousel">
                        <% lectures.forEach((lecture)=> { %>
                            <% if(lecture.data().moduleID===module.id){ %>
                                <div class="vid item">
                                    <div class="thumbnail"> <img src="<%= module.data().thumbnailURL %>" alt=""></div>
                                    <div class="lecName">
                                        <%= lecture.data().lectureOrder %>. <%= lecture.data().lectureName %>
                                    </div>
                                    <div class="lecDesc">
                                        <%= lecture.data().lectureDescription %>
                                    </div>
                                </div>
                                <% } %>
                                    <% }) %>
                    </div>
                </div>
            </div>
            <% }) %>
                <div class="cta">
                    <a href="<%= devFun %>/courseModules/<%= latestCourse.id  %> "><button>View Course
                            Details</button></a>
                </div>
    </div>
